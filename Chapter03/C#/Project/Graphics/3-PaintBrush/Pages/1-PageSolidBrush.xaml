                        <Border Canvas.Top="24" Canvas.Left="50" BorderBrush="Gray" BorderThickness="1">
                            <Rectangle x:Name="rectangleColor" Width="200" Height="36" Fill="White"></Rectangle>
                        </Border>
                        <TextBlock x:Name="txtColor" Canvas.Left="120" Canvas.Top="6" Visibility="Collapsed">#00000000</TextBlock>
                        <TextBlock x:Name="txtColorhash" Canvas.Left="100" FontSize="15"  Foreground="Black" Canvas.Top="6" >#</TextBlock>
                        <TextBlock x:Name="txtColorA" Canvas.Left="110" FontSize="15" Foreground="#EE222222" Canvas.Top="6" >00</TextBlock>
                        <TextBlock x:Name="txtColorR" Canvas.Left="130" FontSize="15" Foreground="Red" Canvas.Top="6">00</TextBlock>

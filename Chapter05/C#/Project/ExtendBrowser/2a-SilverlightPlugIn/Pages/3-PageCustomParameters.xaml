<UserControl x:Class="SilverlightPlugIn.PageCustomParameters"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="400" Height="300">
    <Grid x:Name="LayoutRoot" Background="AliceBlue" Width="400" Height="300" Loaded="LayoutRoot_Loaded" >
        <Canvas Background="White" Width="300" Height="200" >
            <TextBlock Text="Custom Parameters" Canvas.Top="10" Canvas.Left="20" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="24">
            </TextBlock>
            <TextBlock x:Name="txtValues" Canvas.Top="50" Canvas.Left="30">
            </TextBlock>
        </Canvas>
    </Grid>
</UserControl>

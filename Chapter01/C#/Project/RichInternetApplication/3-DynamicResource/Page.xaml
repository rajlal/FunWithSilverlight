<UserControl x:Class="DynamicResource.Page"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="400" Height="300">
    <Grid x:Name="LayoutRoot" Background="#EEEEEE" Width="400" Height="300" >
        <Canvas Background="AliceBlue" Width="300" Height="200" >
            <TextBlock Text="Dynamic Resources" Canvas.Top="10" Canvas.Left="10" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="24">
            </TextBlock>
            <TextBlock Canvas.Top="50" Canvas.Left="20">
                o	XAML Resource<LineBreak/>
                o	Dynamic Images<LineBreak />
                o	Media File<LineBreak />
                o	Audio File<LineBreak />
                o	Embedded Fonts and Font Files<LineBreak />
            </TextBlock>
        </Canvas>
    </Grid>
</UserControl>

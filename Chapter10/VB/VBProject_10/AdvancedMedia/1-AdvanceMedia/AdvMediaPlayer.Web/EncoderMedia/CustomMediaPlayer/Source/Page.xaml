                                            <TextBlock Height="Auto" HorizontalAlignment="Left" Margin="0,0,0,0" Width="auto" Text="{Binding Path=PlaylistIndex}" FontFamily="Arial" Foreground="#FF545454"/>
                                            <TextBlock Height="Auto" Width="auto" Text="." FontFamily="Arial" Foreground="#FF545454"/>
                                        </StackPanel>
                                        <TextBlock Height="Auto" Margin="0,2,2,1.977" Grid.Column="1" Grid.ColumnSpan="2" Text="{Binding Path=Title}" TextWrapping="Wrap" Foreground="#FF545454" FontFamily="Arial"/>

                                                <TextBlock Height="Auto" Width="Auto" FontWeight="Bold" Text="  " Foreground="#FF545454" FontFamily="Arial" TextWrapping="Wrap"/>
                                                <TextBlock Height="Auto" Width="Auto" FontWeight="Bold" Text="{Binding Path=Title}" Foreground="#FF545454" FontFamily="Arial" TextWrapping="Wrap"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </Border>
                                </ContentPresenter.Content>
                            </ContentPresenter>
                            <Rectangle x:Name="FocusVisualElement" Stroke="#FF6DBDD1" StrokeThickness="1" RadiusX="1" RadiusY="1" Visibility="Collapsed"/>
                        </Grid>
